{
	"name": "FPFool",
	"version": "0.0.1",
	"description": "FPFool is an extension to fool fingerprinting websites.",
	"manifest_version": 2,
	"background": {
		"scripts": [
			"./javascripts/background.js",
			"./javascripts/fakeConnections.js"
		],
		"persistent": false
	},
	"browser_action": {
		"default_icon": {
			"16": "res/icon16.png",
			"32": "res/icon32.png",
			"48": "res/icon48.png",
			"128": "res/icon128.png"
		}
	},
	"icons": {
		"16": "res/icon16.png",
		"32": "res/icon32.png",
		"48": "res/icon48.png",
		"128": "res/icon128.png"
	},
	"permissions": [
		"declarativeContent",
		"history",
		"https://*/",
		"http://*/",
		"storage",
		"tabs"
	]
}